{
  "name" : "",
  "hostport" : "localhost:1094",
  "remotemountdir" : "/eos/",
  "localmountdir" : "/eos/",
  "mdcachedir" : "/var/eos/fusex/"
  "mdzmqtarget" : "tcp://localhost:1100",
  "mdzmqidentity" : "eosxd",
  "options" : {
    "debug" : 1,
    "lowleveldebug" : 0,
    "debuglevel" : 5,
    "libfusethreads" : 0,
    "md-kernelcache" : 1,
    "data-kernelcache" : 1,
    "mkdir-is-sync" : 1,
    "create-is-sync" : 1,
    "global-flush" : 1,
    "global-locking" : 1,
    "fd-limit" : 65536,
    "no-fsync" : [ ".db", ".db-journal", ".sqlite", ".sqlite-journal", ".db3", ".db3-journal", "*.o" ]
  },
  "cache" : {
    "type" : "disk",
    "size-mb" : 1000,
    "location" : "/var/eos/fusex/"
    "journal"  : "/var/tmp/fusex/"  
  }
}
